{
  "name": "sirch",
  "version": "1.0.0",
  "description": "centralized search",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "serve": "vite build && node ./src/app/app.js",
    "dev": "concurrently --kill-others --names \"SERVER,SVELTE\" -c \"bgGreen,bgRed\" \"nodemon ./src/app/app.js\" \"vite build --watch\"",
    "dev:ui": "vite",
    "dev:api": "nodemon ./src/app/app.js",
    "build:ui": "vite build",
    "build:api": "echo does not need to build!"
  },
  "keywords": [
    "svelte"
  ],
  "author": "wajeht",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.2.1",
    "nodemon": "^2.0.16"
  },
  "dependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.47",
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cors-anywhere": "^0.4.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "mime": "^3.0.0",
    "svelte": "^3.48.0",
    "svelte-spa-router": "^3.2.0",
    "vite": "^2.9.9"
  }
}
